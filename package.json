{
  "name": "parcel-reason-react-app",
  "version": "1.0.4",
  "description": "Parcel Bundler Example with Reason React",
  "license": "MIT",
  "scripts": {
    "start": "concurrently -c --kill-others 'npm:watch-html' 'npm:watch-reason'",
    "build": "parcel build ./index.html --public-url /",
    "clean": "rimraf .cache lib dist .merlin && bsb -clean-world",
    "watch-html": "parcel ./index.html",
    "watch-reason": "bsb -make-world -w",
    "prestart": "node ./feather.build.js && npx bsrefmt ./src/Feather.re --in-place"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-feather": "^1.1.4",
    "reason-react": "^0.5.3"
  },
  "devDependencies": {
    "bs-platform": "^4.0.5",
    "bsb-js": "^1.1.7",
    "concurrently": "^3.6.1",
    "parcel-bundler": "^1.9.7",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-for": "^2.1.1",
    "postcss-reduce-idents": "^4.0.0",
    "rimraf": "^2.6.2"
  }
}
